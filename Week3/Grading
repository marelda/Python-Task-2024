examscore = int(input("Enter your exam score from (0-100): "))
if examscore >=90 and examscore <=100:
    print ('Grade A')
elif examscore >=80 and examscore <= 89:
    print (' Grade B')
elif examscore >=70 and examscore <= 79:
    print (' Grade C')
elif examscore >=60 and examscore <= 69:
    print (' Grade D')
elif examscore < 60:
    print (' Grade F')    
else:
    print('Invalid Score')
